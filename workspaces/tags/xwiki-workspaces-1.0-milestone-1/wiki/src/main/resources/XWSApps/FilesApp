<createentry_box>#set($workspace=$xwiki.xwsmgr.getSpace())
#set($fName=$util.generateRandomString(12))
#set($rdoc=$xwiki.getDocument("${workspace.spaceName}_Files.${fName}"))
&lt;h1&gt; $msg.get('space.files.attachnewfile')&lt;/h1&gt;
 &lt;form action="$rdoc.getURL('upload')" enctype="multipart/form-data" method="post"&gt;
#set($redirectURL=$xwiki.getDocument("${workspace.spaceName}_Files.WebHome").getURL("view","do=update&amp;which=$rdoc.fullName"))
  &lt;input type="hidden" name="xredirect" value="$redirectURL" /&gt;
   &lt;input id="xwikiuploadname" type="hidden" name="filename" value="" size="40" /&gt;
 &lt;h4&gt;$msg.get('space.files.uploadnewfile')&lt;/h4&gt;
   &lt;input id="xwikiuploadfile" type="file" name="filepath" value="" size="40" /&gt;
   &lt;div&gt;
   &lt;span class="buttonwrapper"&gt;
    &lt;input id="cancel" class="cancel" value="$msg.get('commons.form.cancel')" type="reset" href="#" onclick="javascript:toggleClass($('lb-bg'), 'hidden');toggleClass($('lb-align'), 'hidden');"&gt;
   &lt;/span&gt;
   &lt;span class="buttonwrapper"&gt;
    &lt;input type="submit" value="$msg.get('space.files.addfile')" onclick="return updateAttachName(this.form, '$msg.get('space.files.replacefilename')')" class="button" /&gt;
   &lt;/span&gt;
   &lt;/div&gt;
 &lt;/form&gt;</createentry_box>
